    "@commune-ts/tsconfig": "workspace:*",
    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "eslint": "^8.0.0",
    "prettier": "^3.3.1",
